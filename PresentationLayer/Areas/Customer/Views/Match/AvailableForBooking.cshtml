@using CoreLayer.Helpers
@using DataAccessLayer.Models
@using PresentationLayer.ViewModels
@using System.Globalization
@model ModelsWithPaginationVM<Match>
@{
    ViewData["Title"] = "Available Matches for Booking";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Matches Page</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    <style>
        :root {
            --primary-color: #3d5ee1;
            --secondary-color: #2c3e50;
            --success-color: #28a745;
            --danger-color: #dc3545;
            --warning-color: #ffc107;
            --info-color: #17a2b8;
            --dark-color: #1a1d20;
            --card-bg: #2d3748;
            --text-light: #f8f9fa;
            --border-color: #4a5568;
        }

        body {
            font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
            background-color: #1a202c;
            color: var(--text-light);
        }

        .hero {
            background: linear-gradient(rgba(0, 0, 0, 0.7), rgba(0, 0, 0, 0.7)), url('images/bg_3.jpg') no-repeat center center;
            background-size: cover;
            padding: 100px 0;
            color: white;
            text-align: center;
        }

        .match-score {
            font-size: 3.5rem;
            font-weight: bold;
            background: linear-gradient(135deg, var(--primary-color), #2a4bcc);
            color: white;
            width: 140px;
            height: 140px;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            margin: -70px auto 20px;
            border: 5px solid var(--dark-color);
            box-shadow: 0 5px 15px rgba(0, 0, 0, 0.4);
            z-index: 10;
            position: relative;
        }

        .team-section {
            margin-top: 30px;
            padding: 30px 15px;
        }

        .team-card {
            background: linear-gradient(145deg, #2d3748, #1a202c);
            border-radius: 15px;
            box-shadow: 0 8px 25px rgba(0, 0, 0, 0.25);
            padding: 25px;
            height: 100%;
            transition: transform 0.3s;
            border: 1px solid var(--border-color);
        }

            .team-card:hover {
                transform: translateY(-5px);
                box-shadow: 0 12px 30px rgba(0, 0, 0, 0.35);
            }

        .team-logo {
            max-height: 120px;
            margin: 0 auto 15px;
            display: block;
            transition: transform 0.3s;
            filter: drop-shadow(0 5px 10px rgba(0, 0, 0, 0.5));
        }

        .team-card:hover .team-logo {
            transform: scale(1.05);
        }

        .player-list, .goals-list {
            text-align: left;
            margin-top: 15px;
        }

            .player-list li, .goals-list li {
                padding: 10px 0;
                border-bottom: 1px solid var(--border-color);
                display: flex;
                justify-content: space-between;
                align-items: center;
            }

                .player-list li:last-child, .goals-list li:last-child {
                    border-bottom: none;
                }

        .player-number {
            background: var(--primary-color);
            color: white;
            width: 32px;
            height: 32px;
            border-radius: 50%;
            display: inline-flex;
            align-items: center;
            justify-content: center;
            font-size: 0.8rem;
            margin-right: 12px;
            font-weight: bold;
            box-shadow: 0 2px 5px rgba(0, 0, 0, 0.3);
        }

        .goal-minute {
            background: var(--warning-color);
            color: #000;
            padding: 3px 10px;
            border-radius: 15px;
            font-size: 0.8rem;
            font-weight: bold;
            box-shadow: 0 2px 5px rgba(0, 0, 0, 0.3);
        }

        .widget-next-match {
            background: linear-gradient(135deg, #2c3e50 0%, #1a2530 100%);
            color: white;
            border-radius: 15px;
            padding: 25px;
            box-shadow: 0 8px 20px rgba(0, 0, 0, 0.3);
            border: 1px solid var(--border-color);
        }

        .vs-badge {
            background: var(--primary-color);
            color: white;
            width: 55px;
            height: 55px;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            font-weight: bold;
            margin: 0 auto;
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.3);
            border: 2px solid rgba(255, 255, 255, 0.1);
        }

        .upcoming-match-card {
            background: linear-gradient(145deg, #2d3748, #1a202c);
            border-radius: 12px;
            overflow: hidden;
            box-shadow: 0 8px 20px rgba(0, 0, 0, 0.25);
            transition: all 0.3s ease;
            height: 100%;
            border: 1px solid var(--border-color);
        }

            .upcoming-match-card:hover {
                transform: translateY(-8px);
                box-shadow: 0 15px 30px rgba(0, 0, 0, 0.4);
            }

            .upcoming-match-card .card-body {
                padding: 1.8rem;
            }

        .bg-dark {
            background-color: var(--dark-color) !important;
        }

        .section-title {
            position: relative;
            padding-bottom: 15px;
            margin-bottom: 30px;
            text-align: center;
            color: var(--text-light);
        }

            .section-title:after {
                content: '';
                position: absolute;
                bottom: 0;
                left: 50%;
                transform: translateX(-50%);
                width: 80px;
                height: 3px;
                background: var(--primary-color);
            }

        .text-success {
            color: var(--success-color) !important;
        }

        .text-danger {
            color: var(--danger-color) !important;
        }

        .text-warning {
            color: var(--warning-color) !important;
        }

        .match-info {
            background: rgba(255, 255, 255, 0.08);
            border-radius: 12px;
            padding: 18px;
            margin-top: 20px;
            border: 1px solid var(--border-color);
        }

        .site-section.bg-dark {
            background: linear-gradient(to bottom, #1a202c, #0d1117) !important;
            padding: 60px 0;
        }

        .heading {
            color: var(--text-light);
        }

        .widget-title h3 {
            color: var(--text-light);
            border-bottom: 2px solid var(--primary-color);
            padding-bottom: 10px;
            display: inline-block;
        }

        {
            width: 100px;
            height: 100px;
            font-size: 2.5rem;
            margin-top: -50px;
        }

        .team-logo {
            max-height: 80px;
        }

        .team-card, .upcoming-match-card {
            padding: 20px;
        }

        }

        .btn-launch {
            padding: 15px 30px;
            font-size: 1.2rem;
            font-weight: 600;
            border-radius: 10px;
            box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);
        }

        .modal-content {
            border-radius: 15px;
            overflow: hidden;
            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.2);
        }

        .modal-header {
            background: #3a5e8c;
            color: white;
            padding: 20px;
        }

        .match-selector {
            text-align: center;
            margin-bottom: 20px;
            padding-bottom: 20px;
            border-bottom: 1px dashed #dee2e6;
        }

        .match-teams {
            display: flex;
            justify-content: center;
            align-items: center;
            gap: 20px;
            margin: 20px 0;
        }

        .team-option {
            display: flex;
            flex-direction: column;
            align-items: center;
            cursor: pointer;
            padding: 15px;
            border-radius: 10px;
            transition: all 0.3s ease;
            border: 2px solid transparent;
        }

            .team-option:hover {
                transform: translateY(-5px);
                box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);
            }

            .team-option.active {
                border-color: #3a5e8c;
                background-color: rgba(58, 94, 140, 0.1);
            }

        .team-logo {
            width: 80px;
            height: 80px;
            border-radius: 50%;
            border: 2px solid #3a5e8c;
            padding: 5px;
            margin-bottom: 10px;
            background: white;
            object-fit: contain;
        }

        .vs {
            font-weight: bold;
            font-size: 1.2rem;
            color: #6c757d;
        }

        .match-info {
            background: #f8f9fa;
            border-radius: 10px;
            padding: 15px;
            margin-bottom: 20px;
            border: 1px solid #dee2e6;
        }

        .ticket-category {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 12px 0;
            border-bottom: 1px solid #dee2e6;
        }

            .ticket-category:last-child {
                border-bottom: none;
            }

        .category-info {
            flex: 1;
        }

        .category-name {
            font-weight: 600;
            display: block;
        }

        .category-status {
            font-size: 0.85rem;
            display: block;
        }

        .sold-out {
            color: #dc3545;
        }

        .available {
            color: #28a745;
        }

        .category-price {
            font-weight: 700;
            color: #3a5e8c;
            margin: 0 15px;
        }

        .quantity-controls {
            display: flex;
            align-items: center;
        }

        .quantity-btn {
            width: 32px;
            height: 32px;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            background: #3a5e8c;
            color: white;
            font-weight: bold;
            cursor: pointer;
            user-select: none;
            transition: all 0.2s;
            border: none;
        }

            .quantity-btn:disabled {
                background: #ccc;
                cursor: not-allowed;
            }

            .quantity-btn:not(:disabled):hover {
                background: #2a4a6d;
                transform: scale(1.1);
            }

        .quantity-value {
            margin: 0 12px;
            font-weight: 600;
            min-width: 20px;
            text-align: center;
        }

        .total-section {
            background: #f8f9fa;
            border-radius: 10px;
            padding: 20px;
            margin-top: 25px;
            border: 1px solid #dee2e6;
        }

        .total-row {
            display: flex;
            justify-content: space-between;
            margin-bottom: 10px;
            font-size: 1.1rem;
        }

        .grand-total {
            font-size: 1.4rem;
            font-weight: 800;
            color: #3a5e8c;
            border-top: 2px solid #dee2e6;
            padding-top: 15px;
            margin-top: 15px;
        }

        .limit-notice {
            text-align: center;
            background: #fff9e6;
            padding: 10px;
            border-radius: 8px;
            margin: 15px 0;
            border: 1px solid #ffeeba;
            color: #856404;
            font-weight: 500;
        }

        .modal-footer .btn {
            padding: 10px 20px;
            border-radius: 8px;
            font-weight: 600;
            display: flex;
            align-items: center;
            gap: 8px;
        }

        @@media (max-width: 576px) {
            .match-teams

        {
            flex-direction: column;
            gap: 10px;
        }

        .ticket-category {
            flex-direction: column;
            align-items: flex-start;
            gap: 10px;
        }

        .quantity-controls {
            align-self: flex-end;
        }

        }
        
        .team-logo {
            width: 70px;
            height: 70px;
            cursor: pointer;
            border-radius: 50%;
            transition: 0.2s ease;
        }
        .team-logo:hover {
            transform: scale(1.05);
        }
        .team-logo.selected-team {
            border: 3px solid #007bff;
            background-color:#007bff;
            box-shadow: 0 0 10px rgba(0, 123, 255, 0.6);
        }

    </style>

</head>
<body>

    <!-- Hero Section -->
    <div class="hero overlay" style="background-image: url('/Customer/images/bg_3.jpg');">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-5 mx-auto text-center">
                    <h1 class="text-white">Book Your Tickets</h1>
                    <p>Discover upcoming matches available for fans and book your tickets online.</p>
                </div>
            </div>
        </div>
    </div>

    <!-- Current Match Section -->
    

    <!-- Next Match & Upcoming Matches Section -->
    <div class="site-section bg-dark mt-5">
        <div class="container py-5">

            
            <!-- Upcoming Matches Section -->
            <div class="row">
                <div class="col-12 title-section">
                    <h2 class="heading text-white section-title">Upcoming Matches</h2>
                </div>
                @foreach (var item in Model.Items)
                {
                    <div class="col-lg-6 col-md-6 mb-4">
                        <div class="upcoming-match-card">
                            <div class="card-body">
                                <div class="widget-body">
                                    <div class="widget-vs">
                                        <div class="d-flex align-items-center justify-content-around justify-content-between w-100">
                                            <div class="team-1 text-center">
                                                <img src="@Url.Content("~/assets/images/teamsLogo/" + item.HomeTeam.LogoUrl)"
                                                     alt="@item.HomeTeam.Name" class="img-fluid mb-2" style="max-height: 80px; filter: drop-shadow(0 4px 6px rgba(0,0,0,0.5))">
                                                <h5>@item.HomeTeam.Name</h5>
                                            </div>
                                            <div>
                                                <span class="vs-badge"><span>VS</span></span>
                                            </div>
                                            <div class="team-2 text-center">
                                                <img src="@Url.Content("~/assets/images/teamsLogo/" + item.AwayTeam.LogoUrl)"
                                                     alt="@item.AwayTeam.Name" class="img-fluid mb-2" style="max-height: 80px; filter: drop-shadow(0 4px 6px rgba(0,0,0,0.5))">
                                                <h5>@item.AwayTeam.Name</h5>
                                            </div>
                                           
                                        </div>
                                    </div>
                                </div>

                                <div class="text-center mt-4">
                                    <h5 class="text-primary">@item.Tournament?.Title</h5>
                                    <p class="mb-2">
                                        <span class="d-block">@item.MatchDate.ToString("MMMM dd, yyyy")</span>
                                        <span class="d-block">@item.MatchDate.ToString("hh:mm tt")</span>
                                        <strong class="text-info">@item.Location</strong>
                                    </p>
                                </div>
                                <div style="margin-top:20px; text-align:center">
                                    @if (item.IsAvailableForBooking)
                                    {
                                        <button data-bs-toggle="modal" data-bs-target="#ticketModal_@item.Id" class="btn btn-primary  px-4" style="padding-top:10px; padding-bottom:10px">Book Ticket</button>
                                    }
                                    else
                                    {
                                        <button  class="btn btn-primary  px-4" style="padding-top:10px; padding-bottom:10px" disabled>Book Ticket</button>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                    
                     var modalId = $"ticketModal_{item.Id}";
                    <div class="modal fade" id="@modalId" tabindex="-1" aria-hidden="true">
                        <div class="modal-dialog modal-lg" style="color:black;">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title">
                                        <i class="bi bi-ticket-perforated me-2"></i> Add Ticket To Cart
                                    </h5>
                                    <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
                                </div>
                              <div class="modal-body">
                                    <!-- اختيار الفريق -->
                                    <div id="team-select-@item.Id" class="d-flex justify-content-around  my-3" style="align-items:center">
                                        <div style="width:150px;  text-align:center">
                                             <img src="@Url.Content("~/assets/images/teamsLogo/" + item.HomeTeam.LogoUrl)"
                                             alt="@item.HomeTeam.Name" class="team-logo mx-3"
                                             onclick="selectTeam('@item.Id','Home', '@item.HomeTeam.Name', '@item.HomeTeam.Id')">
                                             <h5>@item.HomeTeam.Name</h5>
                                        </div>
                                         <div class="match-info" style="text-align:center">
                                            <p><strong></strong> @item.Location</p>
                                            <p><strong></strong> @item.MatchDate.ToString("ddd dd MMM yyyy")</p>
                                            <p><strong></strong> @item.MatchDate.ToString("hh:mm tt")</p>
                                        </div>
                                       <div style="width:150px; text-align:center">
                                             <img src="@Url.Content("~/assets/images/teamsLogo/" + item.AwayTeam.LogoUrl)"
                                             alt="@item.AwayTeam.Name" class="team-logo mx-3"
                                             onclick="selectTeam('@item.Id','Away', '@item.AwayTeam.Name', '@item.AwayTeam.Id')">
                                             <h5>@item.AwayTeam.Name</h5>
                                       </div>
                                      
                                    </div>

                                    <p id="selected-team-@item.Id" class="text-center text-primary fw-bold">
                                        Please select a team
                                    </p>

                                    <h6 class="mb-3">Specify the number of tickets</h6>
                                    <div class="limit-notice">
                                        <i class="bi bi-info-circle"></i> You can book up to only 4 tickets per team.
                                    </div>

                                    <div class="ticket-options" id="tickets-@item.Id">
                                        @foreach (var ticket in item.TicketPrices)
                                        {
                                            <div class="ticket-category">
                                                <div class="category-info">
                                                    <span class="category-name">@ticket.Category.GetDisplayName()</span>

                                                    <!-- عدد المتاح لكل فريق -->
                                                    <span class="category-status available"
                                                          id="avail-@item.Id-Home-@ticket.Category"
                                                          style="display:none">
                                                        @ticket.HomeTickets available
                                                    </span>
                                                    <span class="category-status available"
                                                          id="avail-@item.Id-Away-@ticket.Category"
                                                          style="display:none">
                                                        @ticket.AwayTickets available
                                                    </span>

                                                </div>
                                                <span class="category-price">@ticket.Price.ToString("C", new CultureInfo("en-EG"))</span>
                                                <div class="quantity-controls">
                                                    <button class="quantity-btn"
                                                            onclick="updateQuantity('@item.Id','@ticket.Category', -1)">-</button>
                    
                                                    <!-- قيم لكل فريق -->
                                                    <span class="quantity-value"
                                                          id="q-@item.Id-Home-@ticket.Category"
                                                          style="display:none;">0</span>
                                                    <span class="quantity-value"
                                                          id="q-@item.Id-Away-@ticket.Category"
                                                          style="display:none;">0</span>

                                                    <button class="quantity-btn"
                                                            onclick="updateQuantity('@item.Id','@ticket.Category', 1)">+</button>
                                                </div>
                                            </div>
                                        }
                                        <script>
                                            (window._matchInitBuffer = window._matchInitBuffer || []).push({
                                                matchId: '@item.Id',
                                                categories: [
                                                    @foreach (var ticket in item.TicketPrices)
                                                    {
                                                        @: { category: "@ticket.Category", price: @ticket.Price, homeTickets: @ticket.HomeTickets, awayTickets: @ticket.AwayTickets },
                                                    }
                                                ]
                                            });
                                        </script>
                                    </div>

                                    <div class="total-section">
                                        <div class="total-row">
                                            <span>Total:</span>
                                            <span id="subtotal-@item.Id">EGP 0.00</span>
                                        </div>
                                        <div class="total-row">
                                            <span>Tickets:</span>
                                            <span id="tickets-count-@item.Id">0</span>
                                        </div>
                                        <div class="total-row grand-total">
                                            <span>Grand Total:</span>
                                            <span id="grand-total-@item.Id">EGP 0.00</span>
                                        </div>
                                    </div>
                                </div>

                                
                                <div class="modal-footer d-flex justify-content-between">
                                    <button type="button" class="btn btn-outline-secondary" data-bs-dismiss="modal">
                                        <i class="bi bi-x-circle me-1"></i> Cancel
                                    </button>
                                    <button type="button" class="btn btn-success" onclick="addToCart('@item.Id')">
                                        <i class="bi bi-check-circle me-1"></i> Add To Cart
                                    </button>

                                </div>

                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>

  
    <!-- Bootstrap & jQuery JS -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>

 <script>
let ticketData = {}; // { matchId: { Home: {cat:{}}, Away: {cat:{}}, selectedTeam } }

function initMatch(matchId, categories) {
    ticketData[matchId] = { Home: {}, Away: {}, selectedTeam: null };

    categories.forEach(c => {
        ticketData[matchId].Home[c.category] = { price: c.price, quantity: 0, max: c.homeTickets };
        ticketData[matchId].Away[c.category] = { price: c.price, quantity: 0, max: c.awayTickets };
    });
}
function selectTeam(matchId, team, teamName, teamId) {
    // لو غيّر الفريق من قبل
    if (ticketData[matchId].selectedTeam && ticketData[matchId].selectedTeam !== team) {
        // reset الكميات
        Object.keys(ticketData[matchId].Home).forEach(cat => {
            ticketData[matchId].Home[cat].quantity = 0;
            ticketData[matchId].Away[cat].quantity = 0;

            // reset UI quantities
            let qHome = document.getElementById(`q-${matchId}-Home-${cat}`);
            let qAway = document.getElementById(`q-${matchId}-Away-${cat}`);
            if (qHome) qHome.innerText = "0";
            if (qAway) qAway.innerText = "0";

            // إخفاء الـ availability
            let homeAvail = document.getElementById(`avail-${matchId}-Home-${cat}`);
            let awayAvail = document.getElementById(`avail-${matchId}-Away-${cat}`);
            if (homeAvail) homeAvail.style.display = "none";
            if (awayAvail) awayAvail.style.display = "none";
        });

        // reset totals
        document.getElementById(`subtotal-${matchId}`).innerText = "EGP 0.00";
        document.getElementById(`grand-total-${matchId}`).innerText = "EGP 0.00";
        document.getElementById(`tickets-count-${matchId}`).innerText = "0";
    }

    // خزّن الفريق الجديد
    ticketData[matchId].selectedTeam = team;
    ticketData[matchId].selectedTeamId = teamId;

    // تحديث الواجهة (highlight على الفريق المختار)
    document.querySelectorAll(`#team-select-${matchId} .team-logo`).forEach(img => {
        img.classList.remove("selected-team");
    });
    document.querySelector(`#team-select-${matchId} img[onclick*="'${team}']`)
          ?.classList.add("selected-team");

    // إظهار الكاونترات والـ availability الخاصة بالفريق الحالي فقط
    ["Home", "Away"].forEach(t => {
        Object.keys(ticketData[matchId][t]).forEach(cat => {
            let qEl = document.getElementById(`q-${matchId}-${t}-${cat}`);
            let availEl = document.getElementById(`avail-${matchId}-${t}-${cat}`);

            if (qEl) qEl.style.display = (t === team ? "inline" : "none");
            if (availEl) {
                if (t === team) {
                    availEl.style.display = "inline";
                    let max = ticketData[matchId][team][cat].max;
                    availEl.textContent = max > 0 ? "" : "Sold out";
                } else {
                    availEl.style.display = "none";
                }
            }
        });
    });

    // نص الفريق الحالي
    let label = document.getElementById(`selected-team-${matchId}`);
    if (label) {
        label.innerText = `Booking for ${teamName} Team`;
    }
}




function updateQuantity(matchId, category, change) {
    let selectedTeam = ticketData[matchId].selectedTeam;
    if (!selectedTeam) { alert("Please select a team first."); return; }

    let data = ticketData[matchId][selectedTeam][category];
    let totalTickets = Object.values(ticketData[matchId][selectedTeam]).reduce((t, c) => t + c.quantity, 0);

    if (change > 0 && totalTickets >= 4) { alert('Max 4 tickets per team.'); return; }
    if (change > 0 && data.quantity >= data.max) { alert('No more tickets available.'); return; }

    data.quantity = Math.max(0, data.quantity + change);

    // update UI
    document.getElementById(`q-${matchId}-${selectedTeam}-${category}`).textContent = data.quantity;

    let total = 0, count = 0;
    Object.values(ticketData[matchId][selectedTeam]).forEach(c => {
        total += c.quantity * c.price;
        count += c.quantity;
    });

    document.getElementById(`tickets-count-${matchId}`).textContent = count;
    document.getElementById(`subtotal-${matchId}`).textContent = "EGP " + total.toFixed(2);
    document.getElementById(`grand-total-${matchId}`).textContent = "EGP " + total.toFixed(2);
}

  function addToCart(matchId) {
    let team = ticketData[matchId].selectedTeam;
    let teamId = ticketData[matchId].selectedTeamId;
    if (!team) { alert("Please select a team first."); return; }

    let booking = ticketData[matchId][team];
    let cartItems = [];

    for (let category in booking) {
        if (booking[category].quantity > 0) {
            cartItems.push({
                TicketCategory: category,
                Quantity: booking[category].quantity
            });
        }
    }

    if (cartItems.length === 0) { 
        alert("Select at least one ticket."); 
        return; 
    }

    $.ajax({
        url: '/Customer/Cart/Add',
        type: 'POST',
        contentType: 'application/json',
        data: JSON.stringify({
            MatchId: parseInt(matchId),
            TeamId: parseInt(teamId),
            CartItems: cartItems
        }),
        success: function (res) {
            if (res.success) {
                alert(res.message);
                location.reload();
            } else {
                alert(res.message);
            }
        },
        error: function () {
            alert("Error while adding to cart.");
        }
    });
}

function processInitBuffer() {
    (window._matchInitBuffer || []).forEach(x => initMatch(x.matchId, x.categories));
    window._matchInitBuffer = [];
}
document.addEventListener('DOMContentLoaded', processInitBuffer);
</script>


